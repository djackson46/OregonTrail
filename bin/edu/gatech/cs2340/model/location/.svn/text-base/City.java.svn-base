package edu.gatech.cs2340.model.location;

/**
 * Represents a city.
 * 
 * @author The Jetties
 * 
 */
public class City extends Location {

	/**
	 * 
	 */
	private static final long serialVersionUID = -6967148713115166874L;

	/**
	 * Creates a city with the given name and climate.
	 * 
	 * @param name
	 *            The name of the city.
	 * @param climate
	 *            The city's climate
	 */
	public City(String name) {
		super(name);
	}

	public City(String name, int x, int y) {
		super(name, x, y);
	}

}
