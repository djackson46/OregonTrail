package edu.gatech.cs2340.model.human;

/**
 * Represents a banker.
 * 
 * @author The Jetties
 */
public class Banker extends Human {
	/**
	 * 
	 */
	private static final long serialVersionUID = 3186605425338974758L;

	/**
	 * Creates a banker with name [name].
	 * 
	 * @param name
	 *            His name.
	 */
	public Banker(String name) {
		super(name, 25, 320);
		this.career = "Banker";
		this.profession = BANKER;
	}

}
