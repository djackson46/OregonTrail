package edu.gatech.cs2340.model.human;

/**
 * Represents a farmer.
 * 
 * @author The Jetties
 */
public class Farmer extends Human {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2672666581285765801L;

	/**
	 * When you create a carpenter, associate a name with him.
	 * 
	 * @param name
	 *            The name of the carpenter.
	 */
	public Farmer(String name) {
		super(name, 10, 80);
		this.career = "Farmer";
		this.profession = FARMER;
	}
}
