package edu.gatech.cs2340.model;

import java.awt.Graphics;

/**
 * Classes that implement this interface should draw to the supplied Graphics
 * object any graphics that they wish to
 * 
 * @author Jon
 * 
 */
public interface Drawable {
	/**
	 * The implementing object draws to the Graphics object here.
	 * 
	 * @param g
	 *            - graphics object representing the screen
	 */
	public void draw(Graphics g);
}
